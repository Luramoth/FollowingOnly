{
	"manifest_version": 2,
	"name": "Following Only",
	"version": "1.0",
	"description": "Makes twitter bring you to the Following tab no matter what.",
	"icons": {
		"48": "icons/icon32.png",
		"96": "icons/icon96.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://twitter.com/home"
			],
			"js": [
				"FollowingOnly.js"
			]
		}
	]
}